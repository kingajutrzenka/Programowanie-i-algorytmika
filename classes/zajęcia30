def pierwsza(n):
    a = 0
    for i in range(1,n+1):
        if n%i == 0:
          a += 1
    return a == 2

pierwsza(121)

m = str(input("podaj liczbÄ™ naturalnÄ…"))
k = m[::-1]
print(m, k)
print(pierwsza(int(m)) and pierwsza(int(k)))

# zadanie 5

def sympatyczna(m):
  j = []
  while m > 0:
    j.append(m % 10)
    m = m // 10
  j.reverse()
  for i in range(0, len(j) - 1):
     if j[i] >= j[i + 1:
       return False
  return True
n = int(input("n= "))
print(sympatyczna(n))
